generator client {
  provider      = "prisma-client-js"
  output        = "./generated"
  schemaFolders = ["./prisma/generated"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Text {
  id        Int      @id @default(autoincrement())
  fileUrl  String
  fullText  String
  createdAt DateTime @default(now())
}
